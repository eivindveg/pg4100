<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <MenuBar fx:id="menu" BorderPane.alignment="CENTER">
        <menus>
          <Menu fx:id="fileMenu" mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="close" mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu fx:id="helpMenu" mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
      <BorderPane maxHeight="150.0" BorderPane.alignment="CENTER">
         <top>
            <Label fx:id="info" text="Input a number in the box below and press Enter to get information about it." wrapText="true" BorderPane.alignment="CENTER" />
         </top>
         <center>
            <TextField fx:id="input" alignment="CENTER" maxWidth="150.0" prefWidth="100.0" promptText="Input number" BorderPane.alignment="CENTER" />
         </center>
         <bottom>
            <Button defaultButton="true" mnemonicParsing="false" text="Check Number" BorderPane.alignment="CENTER">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </bottom>
      </BorderPane>
   </center>
</BorderPane>
